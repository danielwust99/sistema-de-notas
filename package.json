    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^8.5.4",
        "@types/node": "^16.3.3",
        "@types/uuid": "^8.3.1",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.3.5"
    },
    "dependencies": {
        "bcrypt": "^5.0.1",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "jsonwebtoken": "^8.5.1",
        "pg": "^8.6.0",
        "reflect-metadata": "^0.1.13",
        "ts-node": "^10.1.0",
        "typeorm": "^0.2.34",
        "uuid": "^8.3.2"
    },
    "scripts": {
        "build": "npx tsc",
        "start": "ts-node --files src/server.ts",
        "start:local": "node -r dotenv/config ./out/server",
        "dev": "ts-node-dev --respawn --transpile-only --files ./src/server.ts",
        "migration:run": "ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:run",
        "migration:revert": "ts-node-dev --transpile-only ./node_modules/typeorm/cli.js migration:revert"
    }
